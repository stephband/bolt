
<section class="${ data.type }-doc-toggle-block doc-toggle-block toggle-block block" toggleable id="${ data.type }-${ data.id }" style="background-color: red !important;">
<h3 class="${ data.type }-doctext-05 doctext-05 ${ data.prefix === '--' ? 'doubledash-indent' : data.title && data.title[0] === '"' ? 'quote-indent' : '' }">
    <a href="#${ data.type }-${ data.id }">
        <code class="${ data.type === 'attribute' ? 'token' : '' } ${ data.type === 'property' ? 'property' : '' } ${ data.type === 'attribute' ? 'language-html' : 'language-js' }">${ data.name } ${ data.default && `<span class="default">=<em>${ data.default }</em></span>` || '' }</code>
    </a>
</h3>
</section>
