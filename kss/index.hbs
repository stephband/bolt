        {{#if description}}
          <div class="block grid-1/1">
            {{{description}}}
          </div>
        {{/if ~}}

        {{#if parameters}}
        <div class="block grid-1/1">
          <div class="kss-parameters__title">Parameters:</div>
          <ul class="kss-parameters">
          {{#each parameters}}
            <li class="kss-parameters__item">
              <div class="kss-parameters__name"><code>{{name}}</code></div>
              <div class="kss-parameters__description">
                {{{description}}}
                {{#if defaultValue}}
                  <div class="kss-parameters__default-value">
                    Defaults to: <code>{{defaultValue}}</code>
                  </div>
                {{/if}}
              </div>
            </li>
          {{/each}}
          </ul>
        </div>
        {{/if ~}}


        {{#if example}}<div class="block grid-1/2">
          <!--div class="kss-modifier__heading kss-style">
            Example{{#if modifiers}}s{{/if}}
          </div>

          {{#if modifiers}}
            <div class="kss-modifier__default-name kss-style">
              Default styling
            </div>
          {{/if}}
          -->

          <div class="kss-modifier__example">
            {{{example}}}
            <div class="kss-modifier__example-footer"></div>
          </div>

          {{#each modifiers}}
            <div class="kss-modifier__name kss-style">
              {{name}}
            </div>
            <div class="kss-modifier__description kss-style">
              {{{description}}}
            </div>
            <div class="kss-modifier__example">
              {{{markup}}}
              <div class="kss-modifier__example-footer"></div>
            </div>
          {{/each}}
        </div
      >{{/if}}{{#if markup}}<div class="block grid-1/2">
        <pre class="prettyprint lang-html hidden-scroll scroll"><code data-language="html">{{markup}}</code></pre>
      </div
